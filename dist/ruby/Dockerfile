FROM whatwedo/base:latest
RUN apt-get update -y && \
apt-get install -y ruby ruby-dev bundler && \
apt-get autoremove -y && \
apt-get clean -y && \
rm -rf /var/lib/apt/lists/* /tmp/* /var/tmp/*
LABEL ch.whatwedo.image.base="whatwedo/ruby"
